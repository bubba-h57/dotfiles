# This is Git's per-user configuration file.
[user]
  name = Bubba
  email = rob@stechstudio.com
[push]
  default = upstream
[color]
  diff = auto
  status = auto
  branch = auto
  interactive = auto
  ui = true
  pager = true
[color "status"]
  added = yellow
  changed = green bold
  untracked = cyan bold
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
[core]
  editor = /usr/bin/vim
  autocrlf = input
[help]
  autocorrect = 20
